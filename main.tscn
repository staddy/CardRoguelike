[sub_resource type="Shader" id=9]

code = "shader_type canvas_item;
render_mode blend_mix;

void fragment()
{
	vec4 scol = textureLod(SCREEN_TEXTURE, SCREEN_UV, 0.0);
	vec4 col = textureLod(TEXTURE, UV, 0.0);
    if ((scol.r  != 0.0) || (scol.g  != 0.0) || (scol.b  != 0.0))
        col.a = 0.0;
	else
		col.a /= 8.0;
	col.r = 0.0;
	col.g = 1.0;
	col.b = 1.0;
	COLOR = col;
}"

